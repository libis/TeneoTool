<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="be.libis.teneo.tool.MainController">
   <children>
       <MenuBar fx:id="menuBar" maxWidth="1.7976931348623157E308" VBox.vgrow="NEVER">
           <menus>
               <Menu fx:id="mnuFile" mnemonicParsing="false" text="File">
                   <items>
                       <MenuItem fx:id="miClose" mnemonicParsing="false" text="Close" />
                   </items>
               </Menu>
               <Menu fx:id="mnuEdit" mnemonicParsing="false" text="Edit">
                   <items>
                       <MenuItem fx:id="miDesktopShortcut" mnemonicParsing="false" text="Create desktop shortcut" />
                       <MenuItem fx:id="miMenuShortcut" mnemonicParsing="false" text="Create menu shortcut" />
                   </items>
               </Menu>
               <Menu fx:id="mnuTools" mnemonicParsing="false" text="Tools">
                   <items>
                       <MenuItem fx:id="miMd5" mnemonicParsing="false" text="MD5 Checker" />
                       <MenuItem fx:id="miUpload" mnemonicParsing="false" text="Uploader" />
                   </items>
               </Menu>
               <Menu fx:id="mnuHelp" mnemonicParsing="false" text="Help">
                   <items>
                       <Menu fx:id="mnuLanguage" mnemonicParsing="false" text="Language" />
                       <MenuItem fx:id="miAbout" mnemonicParsing="false" text="About" />
                   </items>
               </Menu>
           </menus>
       </MenuBar>
      <BorderPane fx:id="paneApp" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
